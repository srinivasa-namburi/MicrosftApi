{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.Extensions.Hosting": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Aspire.Hosting": "Information",
      "Aspire.Hosting.Dcp": "Information",
      "Microsoft.Greenlight": "Debug",
      "Azure.Core": "Warning",
      "Azure.Identity": "Warning",
      "Orleans": "Information",
      "Microsoft.EntityFrameworkCore": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
      "LogToStandardErrorThreshold": "Warning"
    }
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "",
    "CallbackPath": "/signin-oidc",
    "ClientId": "",
    "ClientSecret": "",
    "Domain": "",
    "Scopes": ""
  },
  "Azure": {
    "CredentialSource": "AzureCli"
  },
  "ServiceConfiguration": {
    "GreenlightServices": {
      "Scalability": {
        "UseAzureSignalR": false
      },
      "Global": {
        "UseApplicationInsights": false,
        "UsePostgresMemory": true,
        "DeveloperSetupExecuted": true
      },
      "DocumentGeneration": {
        "CreateBodyTextNodes": true
      },
      "DocumentIngestion": {
        "ProcessTables": true,
        "ScheduledIngestion": true
      }
    },
    "SQL": {
      "DatabaseName": "ProjectVicoDB"
    },
    "OpenAI": {
      "GPT4oModelDeploymentName": "gpt-4o",
      "O3MiniModelDeploymentName": "o3-mini"
    }
  },
  "Mcp": {
    "DisableAuth": true
  }
}