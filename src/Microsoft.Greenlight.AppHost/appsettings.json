{
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": "536870912"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning",
      "Azure.Core": "Warning",
      "Microsoft.Greenlight": "Debug"
    }
  },
  "ServiceConfiguration": {
    "GreenlightServices": {
      "Global": {
        "UseApplicationInsights": true,
        "UseAzureSqlServer": false
      },
      "FeatureFlags": {
        "EnableMassDocumentProduction": true,
        "EnableReviews": true,
        "EnableReferenceFrontend": true,
        "EnableContentReferences" : true
      },
      "DocumentValidation": {
        "NumberOfValidationWorkers": "1"
      },
      "DocumentGeneration": {
        "DurableDevelopmentServices": true,
        "CreateBodyTextNodes": true,
        "EmitPromptAndSectionDebugFiles": false,
        "NumberOfGenerationWorkers": "1"
      },
      "DocumentProcesses": [
        {
          "Name": "US.NRC.EnvironmentalReport",
          "ClassifyDocuments": true,
          "IngestionMethod": "KernelMemory",
          "BlobStorageContainerName": "ingest-nrc-environmentalreport",
          "BlobStorageAutoImportFolderName": "ingest-auto",
          "ClassificationModelName": "nrc-classifier",
          "Repositories": [
            "index-us-nrc-envrep-sections"
          ]
        },
        {
          "Name": "Belgium.NuclearLicensing.DSAR",
          "ClassifyDocuments": false,
          "IngestionMethod": "KernelMemory",
          "BlobStorageContainerName": "ingest-belgium-dsar",
          "BlobStorageAutoImportFolderName": "ingest-auto",
          "ClassificationModelName": "belgium-classifier",
          "Repositories": [
            "index-belgium-dsar-sections"
          ]
        }
      ],
      "DocumentIngestion": {
        "NumberOfIngestionWorkers": "1",
        "ProcessTables": true,
        "ScheduledIngestion": true
      },
      "ReferenceIndexing": {
        "RefreshIntervalMinutes": "5"
      }
    },
    "OpenAi": {
      "GPT4128KModelDeploymentName": "gpt-4-128k",
      "GPT432KModelDeploymentName": "gpt-4-32k",
      "GPT4oModelDeploymentName": "gpt-4o",
      "EmbeddingModelDeploymentName": "text-embedding-ada-002"
    },
    "SQL": {
      "DatabaseName": "ProjectVicoDB"
    }
  }
}
