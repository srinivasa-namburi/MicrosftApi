@page "/generate"
@page "/{DocumentProcessNameQueryParameter}/generate"

@code {
    [Parameter] public string? DocumentProcessNameQueryParameter { get; set; }
}

<GenerateForm DocumentProcessName="@DocumentProcessNameQueryParameter"/>

@code {

    protected override async Task OnInitializedAsync()
    {
        if (string.IsNullOrWhiteSpace(DocumentProcessNameQueryParameter))
        {
            DocumentProcessNameQueryParameter = "NRC.Review";
        }
        
    }
}
