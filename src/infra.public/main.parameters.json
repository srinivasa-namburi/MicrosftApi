{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "sqlPassword": {
      "value": "${AZURE_SQL_PASSWORD}"
    },
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "peSubnet": {
      "value": "${AZURE_SUBNET_PE}"
    },
    "containerAppEnvSubnet": {
      "value": "${AZURE_SUBNET_CAE}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "deploymentModel": {
      "value": "${DEPLOYMENT_MODEL}"
    },
    "existingSqlServer": {
      "value": "${EXISTING_SQL_SERVER}"
    }
  }
}
