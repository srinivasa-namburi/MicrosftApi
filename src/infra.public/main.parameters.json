{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "sqlPassword": {
      "value": "${AZURE_SQL_PASSWORD}"
    },
    "resourceGroupName": {
      "value": "${AZURE_RESOURCE_GROUP}"
    },
    "peSubnet": {
      "value": "${AZURE_SUBNET_PE}"
    },
    "containerAppEnvSubnet": {
      "value": "${AZURE_SUBNET_CAE}"
    },
    "postgresSubnet": {
      "value": "${AZURE_SUBNET_POSTGRES}"
    },
    "postgresDnsZoneId": {
      "value": "${POSTGRES_DNSZONE_RESOURCEID}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "deploymentModel": {
      "value": "${DEPLOYMENT_MODEL}"
    },
    "existingSqlServer": {
      "value": "${EXISTING_SQL_SERVER}"
    },
    "administratorPassword": {
      "value": "${BICEP_PG_ADMIN_PASSWORD}"
    },
    "memoryBackend": {
      "value": "${MEMORY_BACKEND}"
    }
  }
}
